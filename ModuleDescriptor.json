{
  "id": "users-bl",
  "name": "users business logic",
  "provides": [
    {
      "id": "users-bl",
      "version": "0.0.1",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/bl-users",
          "permissionsRequired": [ ],
          "permissionsDesired": ["users-bl.read"],
          "modulePermissions": ["users.read", "perms.users.get", "login.readUser"]
        }, {
          "methods": ["GET"],
          "pathPattern": "/bl-users/{id}",
          "permissionsRequired": [],
          "permissionsDesired": ["users-bl.read"],
          "modulePermissions": ["users.read", "perms.users.get", "login.readUser"]
        }, {
          "methods": ["POST"],
          "pathPattern": "/bl-users",
          "permissionsRequired": ["users-bl.create"],
          "modulePermissions": ["users.create", "perms.users.item.post", "login.addUser"]
        }, {
          "methods": ["PUT"],
          "pathPattern": "/bl-users/{id}",
          "permissionsRequired": ["users-bl.modify"],
          "modulePermissions": ["users.edit", "perms.users.item.put", "login.modifyUser"]
        }
      ]
    }
  ],
  "permissionSets": [
    {
      "permissionName": "users-bl.read",
      "displayName": "Read user record",
      "description": "Read user record",
      "subPermissions": ["users.read", "perms.users.get", "login.readUser"]
    }, {
      "permissionName": "users-bl.create",
      "displayName": "Create user record",
      "description": "Can create user record",
      "subPermissions": ["users.create", "perms.users.item.post", "login.addUser"]
    }, {
      "permissionName": "users-bl.modify",
      "displayName": "Modify user record",
      "description": "Can modify user record",
      "subPermissions": ["users.edit", "perms.users.item.put", "login.modifyUser"]
    }, {
      "permissionName": "users-bl.viewuser",
      "displayName": "view user",
      "description": "Can view user profile",
      "subPermissions": ["users-bl.read"]
    }, {
      "permissionName": "users-bl.edituser",
      "displayName": "edit user",
      "description": "Can edit user profile",
      "subPermissions": ["users-bl.read", "users-bl.modify"]
    }, {
      "permissionName": "users-bl.createuser",
      "displayName": "create user",
      "description": "Can create new user",
      "subPermissions": ["users-bl.edituser", "users-bl.create"]
    }
  ]

}
